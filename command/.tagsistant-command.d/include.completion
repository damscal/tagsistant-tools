__tagsistant-tools_include() {
  local commandDir

  # todo 
  if [ "$#" != "5" ]
  then
    commandDir=$1
    tags=`${commandDir}/lstag`
    tagNum=`echo ${tags} | wc -l`
    values=()
    for i in `seq 1 ${tagNum}`
    do
      tagName=`echo ${tags} | sed -n "${i}, ${i}p"`
      values=(${values} "${tagName}")
    done
  
    _values "tags" ${values}
  fi
  return 0;
}